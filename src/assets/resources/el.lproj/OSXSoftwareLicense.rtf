964 \u959  \u940 \u961 \u952 \u961 \u959  48, \u960 \u945 \u961 \u940 \u947 \u961 \u945 \u966 \u959 \u962  12.212 \u942  \u964 \u959  \u940 \u961 \u952 \u961 \u959  48, \u960 \u945 \u961 \u940 \u947 \u961 \u945 \u966 \u959 \u953  227.7202-1 \u969 \u962  227.7202-4, \u954 \u945 \u964 \u940  \u960 \u949 \u961 \u943 \u960 \u964 \u969 \u963 \u951 , \u964 \u959  \u917 \u956 \u960 \u959 \u961 \u953 \u954 \u972  \u923 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  \u933 \u960 \u959 \u955 \u959 \u947 \u953 \u963 \u964 \u974 \u957  \u954 \u945 \u953  \u951  \u932 \u949 \u954 \u956 \u951 \u961 \u943 \u969 \u963 \u951  \u917 \u956 \u960 \u959 \u961 \u953 \u954 \u959 \u973  \u923 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u959 \u973  \u933 \u960 \u959 \u955 \u959 \u947 \u953 \u963 \u964 \u974 \u957  \u960 \u945 \u961 \u941 \u967 \u959 \u957 \u964 \u945 \u953  \u956 \u949  \u940 \u948 \u949 \u953 \u945  \u967 \u961 \u942 \u963 \u951 \u962  \u963 \u949  \u964 \u949 \u955 \u953 \u954 \u959 \u973 \u962  \u967 \u961 \u942 \u963 \u964 \u949 \u962  \u964 \u959 \u965  \u945 \u956 \u949 \u961 \u953 \u954 \u945 \u957 \u953 \u954 \u959 \u973  \u916 \u951 \u956 \u959 \u963 \u943 \u959 \u965  (\u945 ) \u956 \u972 \u957 \u959  \u969 \u962  \u917 \u956 \u960 \u959 \u961 \u953 \u954 \u940  \u917 \u943 \u948 \u951  \u954 \u945 \u953  (\u946 ) \u956 \u972 \u957 \u959  \u956 \u949  \u949 \u954 \u949 \u943 \u957 \u945  \u964 \u945  \u948 \u953 \u954 \u945 \u953 \u974 \u956 \u945 \u964 \u945  \u960 \u959 \u965  \u967 \u959 \u961 \u951 \u947 \u959 \u973 \u957 \u964 \u945 \u953  \u963 \u949  \u972 \u955 \u959 \u965 \u962  \u964 \u959 \u965 \u962  \u940 \u955 \u955 \u959 \u965 \u962  \u964 \u949 \u955 \u953 \u954 \u959 \u973 \u962  \u967 \u961 \u942 \u963 \u964 \u949 \u962  \u963 \u973 \u956 \u966 \u969 \u957 \u945  \u956 \u949  \u964 \u959 \u965 \u962  \u972 \u961 \u959 \u965 \u962  \u960 \u959 \u965  \u960 \u949 \u961 \u953 \u941 \u967 \u959 \u957 \u964 \u945 \u953  \u963 \u964 \u951 \u957  \u960 