\b 13. \uc0\u928 \u955 \u942 \u961 \u951 \u962  \u963 \u965 \u956 \u966 \u969 \u957 \u943 \u945 . \u917 \u960 \u943 \u963 \u951 \u956 \u951  \u947 \u955 \u974 \u963 \u963 \u945 .
\b0  \uc0\u919  \u960 \u945 \u961 \u959 \u973 \u963 \u945  \u902 \u948 \u949 \u953 \u945  \u967 \u961 \u942 \u963 \u951 \u962  \u945 \u957 \u964 \u953 \u960 \u961 \u959 \u963 \u969 \u960 \u949 \u973 \u949 \u953  \u964 \u951 \u957  \u960 \u955 \u942 \u961 \u951  \u963 \u965 \u956 \u966 \u969 \u957 \u943 \u945  \u956 \u949 \u964 \u945 \u958 \u973  \u949 \u963 \u940 \u962  \u954 \u945 \u953  \u964 \u951 \u962  Apple \u945 \u957 \u945 \u966 \u959 \u961 \u953 \u954 \u940  \u956 \u949  \u964 \u951  \u967 \u961 \u942 \u963 \u951  \u964 \u959 \u965  \u923 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u959 \u973  Apple \u954 \u945 \u953  \u965 \u960 \u949 \u961 \u953 \u963 \u967 \u973 \u949 \u953  \u954 \u940 \u952 \u949  \u960 \u961 \u959 \u951 \u947 \u959 \u973 \u956 \u949 \u957 \u951 \u962  \u942  \u964 \u945 \u965 \u964 \u972 \u967 \u961 \u959 \u957 \u951 \u962  \u963 \u965 \u956 \u966 \u969 \u957 \u943 \u945 \u962  \u945 \u957 \u945 \u966 \u959 \u961 \u953 \u954 \u940  \u956 \u949  \u964 \u959  \u943 \u948 \u953 \u959  \u945 \u957 \u964 \u953 \u954 \u949 \u943 \u956 \u949 \u957 \u959 . \u922 \u945 \u956 \u943 \u945  \u964 \u961 \u959 \u960 \u959 \u960 \u959 \u943 \u951 \u963 \u951  \u942  \u956 \u949 \u964 \u945 \u946 \u959 \u955 \u942  \u964 \u951 \u962  \u960 \u945 \u961 \u959 \u973 \u963 \u945 \u962  \u902 \u948 \u949 \u953 \u945 \u962  \u967 \u961 \u942 \u963 \u951 \u962  \u948 \u949 \u957  \u952 \u945  \u949 \u943 \u957 \u945 \u953  \u948 \u949 \u963 \u956 \u949 \u965 \u964 \u953 \u954 \u942  \u949 \u954 \u964 \u972 \u962  \u945 \u957  \u963 \u965 \u957 \u964 \u945 \u967 \u952 \u949 \u943  \u949 \u947 \u947 \u961 \u940 \u966 \u969 \u962  \u954 \u945 \u953  \u965 \u960 \u959 \u947 \u961 \u945 \u966 \u949 \u943  \u945 \u960 \u972  \u964 \u951 \u957  Apple. \u919  \u964 \u965 \u967 \u972 \u957  \u956 \u949 \u964 \u940 \u966 \u961 \u945 \u963 \u951  \u964 \u951 \u962  \u960 \u945 \u961 \u959 \u973 \u963 \u945 \u962  \u902 \u948 \u949 \u953 \u945 \u962  \u967 \u961 \u942 \