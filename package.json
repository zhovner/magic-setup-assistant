{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./app/main.development",
    "dev": "npm run hot-server -- --start-hot",
    "package-linux": "npm run build && build --linux",
    "build-main": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 server.js",
    "postinstall": "npm run build",
    "cleanup": "mop -v"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^8.0.2",
    "@types/react": "^15.0.37",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.1",
    "electron": "~1.6.2",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.28.4",
    "electron-prebuilt": "^0.37.0",
    "express": "^4.13.4",
    "css-modules-require-hook": "^4.0.6",
    "node-sass": "^3.4.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack": "^3.3.0",
    "url-loader": "^0.5.7",
    "ts-loader": "^2.2.0",
    "typescript": "^2.4.1",
    "cross-env": "^5.0.1",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^2.0.0"
  },
  "dependencies": {
    "wifi-control": "^2.0.1"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
